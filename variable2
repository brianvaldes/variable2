#include <stdio.h>
#include <cs50.h>
#include <math.h>

int main(void){
  int a=0;
  int b=0;
  int c=0;
  char unknown [8];
  

  printf("This program is meant to solve the eqaution a*b=c. Choose a variable to be solve: a, b, or c?\n");
  scanf("%c", unknown);

  if(*unknown=='c'){
    a=get_int("what is A?:\n");
    b=get_int("what is B?:\n");
    printf ("C is:%d \n", a*b);
  } else {
  if(*unknown=='a'){
    c=get_int("what is C?:\n");
    b=get_int("what is B?:\n");
    printf ("A is:%d\n",c/b);
  } else {
  if(*unknown=='b'){
    c=get_int("what is C?:\n");
    a=get_int("what is A?:\n");
    printf ("B is:%d\n",c/a);
     
   if(*unknown=='b'){
    c=get_int("what is C?:\n");
    a=get_int("what is A?:\n");
    printf ("B is:%d\n",c/a);
    
  }
}
  }
}
}
